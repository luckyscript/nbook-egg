{
  "name": "nbook-egg",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "require": [
      "egg-ts-helper/register"
    ]
  },
  "scripts": {
    "build": "npm run tsc && node ./scripts/production.js",
    "start": "npm run build && egg-scripts start --daemon --title=egg-server-nbook-egg",
    "stop": "egg-scripts stop --title=egg-server-nbook-egg",
    "docker": "./wait-for-it.sh mysql:3306 -- egg-scripts start --title=egg-server-nbook-egg",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run build",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@luckylab/x-filter": "^1.0.1",
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.10.3",
    "cssnano": "^4.1.10",
    "egg": "^2.6.1",
    "egg-geetest": "^1.0.1",
    "egg-redis": "^2.3.0",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^4.3.1",
    "egg-ts-helper": "^1.11.0",
    "egg-view-ejs": "^2.0.0",
    "fs-extra": "^7.0.1",
    "gt3-sdk": "^2.0.0",
    "highlight.js": "^9.14.2",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^5.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "postcss": "^7.0.14",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "request-promise": "^4.2.4",
    "requests": "^0.2.2",
    "stream-wormhole": "^1.1.0",
    "typescript": "^3.0.0",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "postcss-transform-runtime": "^1.1.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "husky": "^1.3.1",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "",
  "license": "MIT"
}
